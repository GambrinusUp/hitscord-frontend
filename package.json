    "@types/react-redux": "^7.1.34",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lucide-react": "^0.454.0",
    "mediasoup-client": "^3.7.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "sass": "^1.80.6",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
