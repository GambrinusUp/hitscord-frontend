{
  "name": "hitscord-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://GambrinusUp.github.io/hitscord-frontend",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
